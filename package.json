{
  "name": "wtf_wikipedia",
  "description": "parse wikiscript into json",
  "version": "10.0.3",
  "main": "src/index.js",
  "module": "builds/wtf_wikipedia.mjs",
  "unpkg": "builds/wtf_wikipedia-client.min.js",
  "browser": "builds/wtf_wikipedia-client.min.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./builds/wtf_wikipedia.cjs"
    }
  },
  "author": "Spencer Kelly <spencermountain@gmail.com> (http://spencermounta.in)",
  "repository": {
    "type": "git",
    "url": "git://github.com/spencermountain/wtf_wikipedia.git"
  },
  "man": [
    "./builds/man/wtf_wikipedia.1"
  ],
  "types": "types/index.d.ts",
  "scripts": {
    "build": "node ./scripts/version.js && rollup -c",
    "build:all": "node ./scripts/buildAll.js",
    "coverage": "c8 npm run test",
    "lint": "eslint ./src/ ./plugins ./tests",
    "postinstall": "path-exists ./plugins && npm-recursive-install --rootDir=plugins || echo 'nothing to install'",
    "start": "node ./scripts/demo.js",
    "test": "npm run test:fetch && npm run test:integration && npm run test:unit && npm run test:plugins",
    "testb": "cross-env TESTENV=prod npm run test",
    "test:fetch": "tape ./tests/fetch/*.test.js | tap-dancer",
    "test:integration": "tape ./tests/integration/*.test.js | tap-dancer",
    "test:unit": "tape ./tests/unit/**/*.test.js | tap-dancer",
    "test:plugins": "tape ./plugins/*/tests/**/*.test.js | tap-dancer",
    "watch": "amble ./scratch.js"
  },
  "bin": {
    "wtf_wikipedia": "./cli.js"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "browserslist": [
    "node > 12",
    "> 0.2% and not dead"
  ],
  "files": [
    "builds",
    "src",
    "types"
  ],
  "keywords": [
    "wikipedia",
    "wikimedia",
    "wikipedia markup",
    "wikiscript"
  ],
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  },
  "dependencies": {
    "isomorphic-unfetch": "^3.1.0",
    "path-exists-cli": "2.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "23.0.2",
    "@rollup/plugin-node-resolve": "15.0.1",
    "amble": "1.3.0",
    "c8": "^7.12.0",
    "cross-env": "^7.0.3",
    "eslint": "8.26.0",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-jsdoc": "^39.3.23",
    "eslint-plugin-regexp": "1.9.0",
    "recursive-install": "https://github.com/emgeee/recursive-install",
    "rollup": "2.79.1",
    "rollup-plugin-terser": "7.0.2",
    "shelljs": "0.8.5",
    "tap-dancer": "0.3.4",
    "tape": "5.6.1"
  },
  "eslintIgnore": [
    "builds/*.js"
  ],
  "license": "MIT"
}
